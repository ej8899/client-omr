  "elmType": "div",
  "txtContent": "=if([$RenewalDate] > (@now + 21038400000), 'GOOD', if([$RenewalDate] > (@now + 5259600000), 'Expiring', if([$RenewalDate] > @now, 'Expiring Soon', 'Expired')))",
